<Window
    x:Class="Asphalt_9_Materials.UI.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:aboutPages="clr-namespace:Asphalt_9_Materials.UI.Pages.About"
    xmlns:availableCarsPages="clr-namespace:Asphalt_9_Materials.UI.Pages.AvailableCars"
    xmlns:blueprintsPages="clr-namespace:Asphalt_9_Materials.UI.Pages.Blueprints"
    xmlns:careerPages="clr-namespace:Asphalt_9_Materials.UI.Pages.Career"
    xmlns:con="clr-namespace:Asphalt_9_Materials.ViewModel.Converters;assembly=Asphalt9Materials.ViewModel"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:introPages="clr-namespace:Asphalt_9_Materials.UI.Pages.Introduction"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:miscComparisonPages="clr-namespace:Asphalt_9_Materials.UI.Pages.Misc.Comparison"
    xmlns:miscPages="clr-namespace:Asphalt_9_Materials.UI.Pages.Misc"
    xmlns:performancePages="clr-namespace:Asphalt_9_Materials.UI.Pages.Performance"
    xmlns:rankPages="clr-namespace:Asphalt_9_Materials.UI.Pages.Ranks"
    xmlns:routePages="clr-namespace:Asphalt_9_Materials.UI.Pages.Routes"
    xmlns:system="clr-namespace:System;assembly=mscorlib"
    xmlns:vmAbout="clr-namespace:Asphalt_9_Materials.ViewModel.ViewModels.About;assembly=Asphalt9Materials.ViewModel"
    xmlns:vmAvailableCars="clr-namespace:Asphalt_9_Materials.ViewModel.ViewModels.AvailableCars;assembly=Asphalt9Materials.ViewModel"
    xmlns:vmBlueprints="clr-namespace:Asphalt_9_Materials.ViewModel.ViewModels.Blueprints;assembly=Asphalt9Materials.ViewModel"
    xmlns:vmCareer="clr-namespace:Asphalt_9_Materials.ViewModel.ViewModels.Career;assembly=Asphalt9Materials.ViewModel"
    xmlns:vmIntroduction="clr-namespace:Asphalt_9_Materials.ViewModel.ViewModels.Introduction;assembly=Asphalt9Materials.ViewModel"
    xmlns:vmMisc="clr-namespace:Asphalt_9_Materials.ViewModel.ViewModels.Misc;assembly=Asphalt9Materials.ViewModel"
    xmlns:vmMiscComparison="clr-namespace:Asphalt_9_Materials.ViewModel.ViewModels.Misc.Comparison;assembly=Asphalt9Materials.ViewModel"
    xmlns:vmPerformance="clr-namespace:Asphalt_9_Materials.ViewModel.ViewModels.Performance.Performance;assembly=Asphalt9Materials.ViewModel"
    xmlns:vmRanks="clr-namespace:Asphalt_9_Materials.ViewModel.ViewModels.Ranks;assembly=Asphalt9Materials.ViewModel"
    xmlns:vmTracks="clr-namespace:Asphalt_9_Materials.ViewModel.ViewModels.Routes;assembly=Asphalt9Materials.ViewModel"
    x:Name="MainView"
    Title="MainWindow"
    Width="1700"
    Height="1030"
    MaxWidth="1700"
    MaxHeight="1030"
    AllowsTransparency="True"
    Background="Transparent"
    MouseLeftButtonDown="Window_MouseLeftButtonDown"
    ResizeMode="NoResize"
    SnapsToDevicePixels="True"
    WindowStartupLocation="CenterScreen"
    WindowState="{Binding WindowState, Mode=TwoWay}"
    WindowStyle="None"
    mc:Ignorable="d">

    <Window.Resources>
        <!--#region Converters-->
        <con:ToLowerCaseConverter x:Key="ToLowerCase" />
        <!--#endregion-->
        <!--#region Pages-->
        <DataTemplate DataType="{x:Type vmAbout:AboutViewModel}">
            <aboutPages:AboutPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmAbout:CreditsViewModel}">
            <aboutPages:CreditsPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmAvailableCars:CarsPageViewModel}">
            <availableCarsPages:CarsPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmIntroduction:TipsViewModel}">
            <introPages:TipsPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmIntroduction:IntroductionViewModel}">
            <introPages:IntroductionPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmIntroduction:HowToViewModel}">
            <introPages:HowToPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmIntroduction:CurrenciesViewModel}">
            <introPages:CurrenciesPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmIntroduction:GamePlayViewModel}">
            <introPages:GamePlayPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmMisc:AddNewViewModel}">
            <miscPages:AddNewCar />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmMisc:DeleteCarViewModel}">
            <miscPages:DeletePage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmCareer:CareerViewModel}">
            <careerPages:CareerPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmMisc:UpdateCarViewModel}">
            <miscPages:UpdateCarPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmMisc:ImportantNoteViewModel}">
            <miscPages:ImportantNotePage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmMiscComparison:MaxComparisonViewModel}">
            <miscComparisonPages:MaxComparisonPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmMiscComparison:StockComparisonViewModel}">
            <miscComparisonPages:StockComparisonPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmMiscComparison:RankComparisonViewModel}">
            <miscComparisonPages:RankComparisonPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmMiscComparison:BlueprintsComparisonViewModel}">
            <miscComparisonPages:BlueprintsComparisonPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmBlueprints:BlueprintsViewModel}">
            <blueprintsPages:BlueprintsPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmPerformance:MaxPerformanceViewModel}">
            <performancePages:MaxPerformancePage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmPerformance:StockPerformanceViewModel}">
            <performancePages:StockPerformancePage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmRanks:StarRanksViewModel}">
            <rankPages:RanksPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmTracks:TracksViewModel}">
            <routePages:RoutesPage />
        </DataTemplate>
        <!--#endregion-->
    </Window.Resources>

    <!--#region Fading Window Effects-->

    <Window.Triggers>
        <EventTrigger RoutedEvent="Window.Loaded">
            <BeginStoryboard>
                <Storyboard x:Name="WindowStartupAnimate">
                    <DoubleAnimation
                        AutoReverse="False"
                        RepeatBehavior="1x"
                        Storyboard.TargetName="MainView"
                        Storyboard.TargetProperty="Opacity"
                        From="0.0"
                        To="1.0"
                        Duration="0:0:1" />
                </Storyboard>
            </BeginStoryboard>
        </EventTrigger>
    </Window.Triggers>

    <!--#endregion-->

    <materialDesign:DialogHost Background="{StaticResource DarkBrush}" BorderBrush="{x:Null}">
        <materialDesign:DialogHost.DialogContent>
            <Grid
                Width="350"
                Height="150"
                HorizontalAlignment="Center"
                Background="{StaticResource DarkBrush}">
                <StackPanel Margin="15" Orientation="Horizontal">
                    <materialDesign:PackIcon
                        Width="50"
                        Height="50"
                        Foreground="{StaticResource PrimaryHueMidBrush}"
                        Kind="Close" />
                    <TextBlock
                        Margin="15,15"
                        FontSize="18"
                        Foreground="{StaticResource LightBrush}"
                        TextWrapping="Wrap">
                        Are you sure want to exit...?
                    </TextBlock>
                </StackPanel>
                <StackPanel
                    Margin="15"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Bottom"
                    Orientation="Horizontal">
                    <Button
                        Margin="4"
                        VerticalAlignment="Center"
                        Command="{x:Static materialDesign:DialogHost.CloseDialogCommand}"
                        Style="{DynamicResource MaterialDesignFlatButton}">
                        Cancel
                    </Button>
                    <Button
                        Margin="4"
                        VerticalAlignment="Center"
                        Command="{Binding CloseApplicationCommand}"
                        Style="{DynamicResource MaterialDesignFlatButton}">
                        Yes
                    </Button>
                </StackPanel>
            </Grid>
        </materialDesign:DialogHost.DialogContent>
        <Grid Height="1030">
            <!--#region Rows & Columns Definitions-->
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="330" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="100" />
                <RowDefinition Height="auto" />
                <RowDefinition Height="70" />
            </Grid.RowDefinitions>
            <!--#endregion-->
            <!--#region Grid Background-->
            <Grid.Background>
                <ImageBrush Opacity=".2" Stretch="fill">
                    <ImageBrush.ImageSource>
                        <BitmapImage
                            DecodePixelHeight="1030"
                            DecodePixelWidth="1700"
                            UriSource="Assets/bg.jpg" />
                    </ImageBrush.ImageSource>
                </ImageBrush>
            </Grid.Background>
            <!--#endregion-->
            <!--#region Header-->
            <Grid
                x:Name="HeaderGrid"
                Grid.Row="0"
                Grid.Column="1"
                Width="1370"
                Height="100"
                HorizontalAlignment="Right"
                VerticalAlignment="Top">
                <DockPanel
                    x:Name="HeaderDock"
                    Width="1370"
                    HorizontalAlignment="Right"
                    Background="{StaticResource DarkerBrush}">
                    <materialDesign:PackIcon
                        Width="32"
                        Height="32"
                        Margin="0,0,30,0"
                        VerticalAlignment="Center"
                        Cursor="Hand"
                        DockPanel.Dock="Right"
                        Foreground="{StaticResource LightBrush}"
                        Kind="Close"
                        ToolTip="Closing Application">
                        <materialDesign:PackIcon.InputBindings>
                            <MouseBinding Command="{x:Static materialDesign:DialogHost.OpenDialogCommand}" Gesture="LeftClick" />
                        </materialDesign:PackIcon.InputBindings>
                    </materialDesign:PackIcon>
                    <materialDesign:PackIcon
                        Width="32"
                        Height="24"
                        Margin="0,0,7,0"
                        VerticalAlignment="Center"
                        Cursor="Hand"
                        DockPanel.Dock="Right"
                        Foreground="{StaticResource LightBrush}"
                        Kind="WindowMinimize"
                        ToolTip="Minimize Application">
                        <materialDesign:PackIcon.InputBindings>
                            <MouseBinding Command="{Binding MinimizeApplicationCommand}" Gesture="LeftClick" />
                        </materialDesign:PackIcon.InputBindings>
                    </materialDesign:PackIcon>
                    <Viewbox
                        Width="1"
                        Height="28"
                        Margin="0,0,8,0"
                        VerticalAlignment="Center"
                        DockPanel.Dock="Right">
                        <Border BorderBrush="{StaticResource SeparatorBrush}" BorderThickness="1,28" />
                    </Viewbox>
                    <materialDesign:PopupBox
                        Margin="0,0,10,0"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        Background="Transparent"
                        BorderBrush="Transparent"
                        DockPanel.Dock="Right"
                        Foreground="{StaticResource LightBrush}"
                        IsPopupOpen="False"
                        PlacementMode="BottomAndAlignRightEdges"
                        Style="{StaticResource MaterialDesignPopupBox}">
                        <materialDesign:PopupBox.ToggleContent>
                            <materialDesign:PackIcon
                                Width="28"
                                Height="28"
                                Foreground="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=materialDesign:PopupBox}, Path=Foreground}"
                                Kind="Gear" />
                        </materialDesign:PopupBox.ToggleContent>
                        <materialDesign:PopupBox.ToggleCheckedContent>
                            <materialDesign:PackIcon
                                Width="28"
                                Height="28"
                                Foreground="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=materialDesign:PopupBox}, Path=Foreground}"
                                Kind="GearOutline" />
                        </materialDesign:PopupBox.ToggleCheckedContent>
                        <ListBox Height="auto">
                            <ListBoxItem HorizontalContentAlignment="Center">
                                <TextBlock
                                    x:Name="ExportDbX"
                                    Padding="0,4"
                                    Cursor="Hand"
                                    FontFamily="{DynamicResource RajdhaniMedium}"
                                    FontSize="15"
                                    Foreground="{StaticResource LightBrush}"
                                    Text="Export Database">
                                    <TextBlock.InputBindings>
                                        <MouseBinding Command="{Binding ExportDatabaseCommand}" Gesture="LeftClick" />
                                    </TextBlock.InputBindings>
                                </TextBlock>
                            </ListBoxItem>
                        </ListBox>
                    </materialDesign:PopupBox>
                </DockPanel>
            </Grid>
            <!--#endregion-->
            <!--#region  Logo-->
            <Grid
                x:Name="LogoGrid"
                Grid.Row="0"
                Grid.RowSpan="2"
                Grid.Column="0"
                Width="330"
                Height="220"
                Margin="0"
                Background="{StaticResource NavBrush}">
                <Rectangle
                    x:Name="LogoImage"
                    Width="290"
                    Height="200"
                    Margin="-15,0,0,0">
                    <Rectangle.Fill>
                        <ImageBrush>
                            <ImageBrush.ImageSource>
                                <BitmapImage
                                    DecodePixelHeight="200"
                                    DecodePixelWidth="290"
                                    UriSource="Assets/AsphaltMaterials.png" />
                            </ImageBrush.ImageSource>
                        </ImageBrush>
                    </Rectangle.Fill>
                </Rectangle>
            </Grid>
            <!--#endregion-->
            <!--#region Footer-->
            <Grid
                x:Name="FooterGrid"
                Grid.Row="2"
                Grid.Column="1"
                Height="70"
                VerticalAlignment="Bottom">
                <DockPanel
                    x:Name="StatusBarDock"
                    Background="{StaticResource DarkerBrush}"
                    LastChildFill="False">
                    <TextBlock
                        x:Name="TimeTb"
                        Width="94"
                        Margin="0,25,10,25"
                        VerticalAlignment="Center"
                        DockPanel.Dock="Right"
                        FontFamily="{StaticResource RajdhaniSemiBold}"
                        FontSize="20"
                        Foreground="{StaticResource LightBrush}" />
                </DockPanel>
            </Grid>
            <!--#endregion-->
            <!--#region Navbar-->
            <Grid
                x:Name="NavGrid"
                Grid.Row="1"
                Grid.RowSpan="2"
                Grid.Column="0"
                Width="330"
                Height="810"
                Background="{StaticResource NavBrush}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="810" />
                    <RowDefinition Height="0" />
                </Grid.RowDefinitions>
                <!--#region ScrollViewer-->
                <ScrollViewer
                    x:Name="NavScrollViewer"
                    Margin="0,0,0,67"
                    VerticalAlignment="Stretch"
                    HorizontalContentAlignment="Stretch"
                    VerticalContentAlignment="Stretch"
                    Background="{x:Null}"
                    CanContentScroll="True"
                    HorizontalScrollBarVisibility="Disabled"
                    SnapsToDevicePixels="True"
                    VerticalScrollBarVisibility="Visible">
                    <StackPanel Height="810">
                        <!--#region Introduction-->
                        <Expander
                            x:Name="IntroExpander"
                            Margin="0"
                            Padding="2"
                            HorizontalContentAlignment="Stretch"
                            Background="{x:Null}"
                            Cursor="Hand"
                            FontFamily="{StaticResource RajdhaniMedium}"
                            FontSize="18"
                            Foreground="{DynamicResource LightBrush}"
                            TabIndex="1">
                            <Expander.Header>
                                <StackPanel
                                    Margin="10,0,0,0"
                                    HorizontalAlignment="Left"
                                    Orientation="Horizontal">
                                    <Viewbox
                                        Width="24"
                                        Height="24"
                                        Margin="0,-6,0,0"
                                        RenderTransformOrigin="0.5,0.5">
                                        <materialDesign:PackIcon
                                            Width="21"
                                            Height="20"
                                            Margin="0,0,0,-7"
                                            VerticalAlignment="Stretch"
                                            Foreground="{StaticResource LightBrush}"
                                            Kind="Comments" />
                                    </Viewbox>
                                    <TextBlock
                                        Margin="15,3,0,0"
                                        FontFamily="{DynamicResource RajdhaniSemiBold}"
                                        Text="INTRODUCTION" />
                                </StackPanel>
                            </Expander.Header>
                            <StackPanel>
                                <ItemsControl HorizontalContentAlignment="Center">
                                    <Button
                                        x:Name="IntroPage"
                                        Height="45"
                                        Margin="0"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding ShowIntroductionCommand}"
                                        CommandParameter="{Binding Tag, ElementName=IntroPage}"
                                        FontSize="16"
                                        Foreground="{DynamicResource LightBrush}"
                                        Style="{StaticResource MaterialDesignFlatButton}"
                                        Tag="Intro|Introduction">
                                        <StackPanel
                                            Margin="60,0,0,0"
                                            HorizontalAlignment="Left"
                                            Orientation="Horizontal">
                                            <Viewbox Width="16" Height="16">
                                                <materialDesign:PackIcon
                                                    Width="48"
                                                    Height="48"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Top"
                                                    Kind="CommentTextMultiple" />
                                            </Viewbox>
                                            <TextBlock
                                                Margin="8,0,0,0"
                                                FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                FontWeight="Normal"
                                                Text="Introduction" />
                                        </StackPanel>
                                    </Button>
                                    <Button
                                        x:Name="GamePlayBtn"
                                        Height="45"
                                        Margin="0"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding ShowIntroductionCommand}"
                                        CommandParameter="{Binding Tag, ElementName=GamePlayBtn}"
                                        FontSize="16"
                                        Foreground="{DynamicResource LightBrush}"
                                        Style="{StaticResource MaterialDesignFlatButton}"
                                        Tag="Game_Play|Introduction">
                                        <StackPanel
                                            Margin="60,0,0,0"
                                            HorizontalAlignment="Left"
                                            Orientation="Horizontal">
                                            <Viewbox Width="16" Height="16">
                                                <materialDesign:PackIcon
                                                    Width="48"
                                                    Height="48"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Top"
                                                    Kind="CommentTextMultiple" />
                                            </Viewbox>
                                            <TextBlock
                                                Margin="8,0,0,0"
                                                FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                FontWeight="Normal"
                                                Text="GamePlay" />
                                        </StackPanel>
                                    </Button>
                                    <Button
                                        x:Name="CurrenciesBtn"
                                        Height="45"
                                        Margin="0"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding ShowIntroductionCommand}"
                                        CommandParameter="{Binding Tag, ElementName=CurrenciesBtn}"
                                        FontSize="16"
                                        Foreground="{DynamicResource LightBrush}"
                                        Style="{StaticResource MaterialDesignFlatButton}"
                                        Tag="Currencies|Introduction">
                                        <StackPanel
                                            Margin="60,0,0,0"
                                            HorizontalAlignment="Left"
                                            Orientation="Horizontal">
                                            <Viewbox Width="16" Height="16">
                                                <materialDesign:PackIcon
                                                    Width="48"
                                                    Height="48"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Top"
                                                    Kind="CommentTextMultiple" />
                                            </Viewbox>
                                            <TextBlock
                                                Margin="8,0,0,0"
                                                FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                FontWeight="Normal"
                                                Text="Currencies" />
                                        </StackPanel>
                                    </Button>
                                    <Button
                                        x:Name="HowToBtn"
                                        Height="45"
                                        Margin="0"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding ShowIntroductionCommand}"
                                        CommandParameter="{Binding Tag, ElementName=HowToBtn}"
                                        FontSize="16"
                                        Foreground="{DynamicResource LightBrush}"
                                        Style="{StaticResource MaterialDesignFlatButton}"
                                        Tag="How_To|Introduction">
                                        <StackPanel
                                            Margin="60,0,0,0"
                                            HorizontalAlignment="Left"
                                            Orientation="Horizontal">
                                            <Viewbox Width="16" Height="16">
                                                <materialDesign:PackIcon
                                                    Width="48"
                                                    Height="48"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Top"
                                                    Kind="CommentTextMultiple" />
                                            </Viewbox>
                                            <TextBlock
                                                Margin="8,0,0,0"
                                                FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                FontWeight="Normal"
                                                Text="How To...?" />
                                        </StackPanel>
                                    </Button>
                                    <Button
                                        x:Name="TipsBtn"
                                        Height="45"
                                        Margin="0"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding ShowIntroductionCommand}"
                                        CommandParameter="{Binding Tag, ElementName=TipsBtn}"
                                        FontSize="16"
                                        Foreground="{DynamicResource LightBrush}"
                                        Style="{StaticResource MaterialDesignFlatButton}"
                                        Tag="Tips|Introduction">
                                        <StackPanel
                                            Margin="60,0,0,0"
                                            HorizontalAlignment="Left"
                                            Orientation="Horizontal">
                                            <Viewbox Width="16" Height="16">
                                                <materialDesign:PackIcon
                                                    Width="48"
                                                    Height="48"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Top"
                                                    Kind="CommentTextMultiple" />
                                            </Viewbox>
                                            <TextBlock
                                                Margin="8,0,0,0"
                                                FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                FontWeight="Normal"
                                                Text="Tips And Trick" />
                                        </StackPanel>
                                    </Button>
                                </ItemsControl>
                            </StackPanel>
                        </Expander>
                        <!--#endregion-->
                        <!--#region Cars Available-->
                        <Expander
                            x:Name="CarExpander"
                            Margin="0"
                            Padding="2"
                            HorizontalContentAlignment="Stretch"
                            Background="{x:Null}"
                            Cursor="Hand"
                            FontFamily="{StaticResource RajdhaniMedium}"
                            FontSize="18"
                            Foreground="{DynamicResource LightBrush}"
                            TabIndex="1">
                            <Expander.Header>
                                <StackPanel
                                    Margin="10,0,0,0"
                                    HorizontalAlignment="Left"
                                    Orientation="Horizontal">
                                    <Viewbox Width="24" Height="24">
                                        <Path
                                            Margin="0,-10,0,0"
                                            HorizontalAlignment="Left"
                                            Data="{StaticResource AvailableCarsIcon}"
                                            Fill="{DynamicResource LightBrush}" />
                                    </Viewbox>
                                    <TextBlock
                                        Margin="15,3,0,0"
                                        FontFamily="{DynamicResource RajdhaniSemiBold}"
                                        Text="AVAILABLE CARS" />
                                </StackPanel>
                            </Expander.Header>
                            <StackPanel>
                                <ItemsControl HorizontalContentAlignment="Center">
                                    <Button
                                        x:Name="ClassDCars"
                                        Height="45"
                                        Margin="0"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding CollectCarsCommand}"
                                        CommandParameter="{Binding Tag, ElementName=ClassDCars}"
                                        FontSize="16"
                                        Foreground="{DynamicResource LightBrush}"
                                        Style="{StaticResource MaterialDesignFlatButton}"
                                        Tag="D|Cars">
                                        <StackPanel
                                            Margin="60,0,0,0"
                                            HorizontalAlignment="Left"
                                            Orientation="Horizontal">
                                            <Viewbox Width="16" Height="16">
                                                <materialDesign:PackIcon
                                                    Width="48"
                                                    Height="48"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Top"
                                                    Kind="CarBack" />
                                            </Viewbox>
                                            <TextBlock
                                                Margin="8,0,0,0"
                                                FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                FontWeight="Normal"
                                                Text="Class D" />
                                        </StackPanel>
                                    </Button>
                                    <Button
                                        x:Name="ClassCCars"
                                        Height="45"
                                        Margin="0"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding CollectCarsCommand}"
                                        CommandParameter="{Binding Tag, ElementName=ClassCCars}"
                                        FontSize="16"
                                        Foreground="{DynamicResource LightBrush}"
                                        Style="{StaticResource MaterialDesignFlatButton}"
                                        Tag="C|Cars">
                                        <StackPanel
                                            Margin="60,0,0,0"
                                            HorizontalAlignment="Left"
                                            Orientation="Horizontal">
                                            <Viewbox Width="16" Height="16">
                                                <materialDesign:PackIcon
                                                    Width="48"
                                                    Height="48"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Top"
                                                    Kind="CarBack" />
                                            </Viewbox>
                                            <TextBlock
                                                Margin="8,0,0,0"
                                                FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                FontWeight="Normal"
                                                Text="Class C" />
                                        </StackPanel>
                                    </Button>
                                    <Button
                                        x:Name="ClassBCars"
                                        Height="45"
                                        Margin="0"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding CollectCarsCommand}"
                                        CommandParameter="{Binding Tag, ElementName=ClassBCars}"
                                        FontSize="16"
                                        Foreground="{DynamicResource LightBrush}"
                                        Style="{StaticResource MaterialDesignFlatButton}"
                                        Tag="B|Cars">
                                        <StackPanel
                                            Margin="60,0,0,0"
                                            HorizontalAlignment="Left"
                                            Orientation="Horizontal">
                                            <Viewbox Width="16" Height="16">
                                                <materialDesign:PackIcon
                                                    Width="48"
                                                    Height="48"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Top"
                                                    Kind="CarBack" />
                                            </Viewbox>
                                            <TextBlock
                                                Margin="8,0,0,0"
                                                FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                FontWeight="Normal"
                                                Text="Class B" />
                                        </StackPanel>
                                    </Button>
                                    <Button
                                        x:Name="ClassACars"
                                        Height="45"
                                        Margin="0"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding CollectCarsCommand}"
                                        CommandParameter="{Binding Tag, ElementName=ClassACars}"
                                        FontSize="16"
                                        Foreground="{DynamicResource LightBrush}"
                                        Style="{StaticResource MaterialDesignFlatButton}"
                                        Tag="A|Cars">
                                        <StackPanel
                                            Margin="60,0,0,0"
                                            HorizontalAlignment="Left"
                                            Orientation="Horizontal">
                                            <Viewbox Width="16" Height="16">
                                                <materialDesign:PackIcon
                                                    Width="48"
                                                    Height="48"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Top"
                                                    Kind="CarBack" />
                                            </Viewbox>
                                            <TextBlock
                                                Margin="8,0,0,0"
                                                FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                FontWeight="Normal"
                                                Text="Class A" />
                                        </StackPanel>
                                    </Button>
                                    <Button
                                        x:Name="ClassSCars"
                                        Height="45"
                                        Margin="0"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding CollectCarsCommand}"
