                                            <Viewbox Width="16" Height="16">
                                                <materialDesign:PackIcon
                                                    Width="48"
                                                    Height="48"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Top"
                                                    Kind="CarOff" />
                                            </Viewbox>
                                            <TextBlock
                                                Margin="8,0,0,0"
                                                FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                Text="Delete" />
                                        </StackPanel>
                                    </Button>
                                    <Expander
                                        x:Name="Comparing"
                                        Margin="0"
                                        Padding="2"
                                        HorizontalContentAlignment="Stretch"
                                        Background="{x:Null}"
                                        Cursor="Hand"
                                        FontFamily="{StaticResource RajdhaniMedium}"
                                        FontSize="16"
                                        Foreground="{DynamicResource LightBrush}"
                                        TabIndex="6">
                                        <Expander.Header>
                                            <StackPanel
                                                Margin="50,0,0,0"
                                                HorizontalAlignment="Left"
                                                Orientation="Horizontal">
                                                <Viewbox Width="16" Height="16">
                                                    <materialDesign:PackIcon
                                                        Width="48"
                                                        Height="48"
                                                        FontSize="16"
                                                        Kind="Ballot" />
                                                </Viewbox>
                                                <TextBlock
                                                    Margin="8,3,0,0"
                                                    FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                    FontSize="16"
                                                    Text="Compare " />
                                            </StackPanel>
                                        </Expander.Header>
                                        <StackPanel>
                                            <Button
                                                x:Name="CompareStockBtn"
                                                Height="45"
                                                Margin="0"
                                                HorizontalContentAlignment="Stretch"
                                                Command="{Binding ShowMiscCommand}"
                                                CommandParameter="{Binding Path=Tag, ElementName=CompareStockBtn}"
                                                FontSize="16"
                                                Foreground="{DynamicResource LightBrush}"
                                                Style="{StaticResource MaterialDesignFlatButton}"
                                                Tag="CarComparison|Misc|Stock">
                                                <StackPanel
                                                    Margin="70,0,0,0"
                                                    HorizontalAlignment="Left"
                                                    Orientation="Horizontal">
                                                    <Viewbox Width="16" Height="16">
                                                        <materialDesign:PackIcon
                                                            Width="48"
                                                            Height="48"
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Top"
                                                            Kind="CarConnected" />
                                                    </Viewbox>
                                                    <TextBlock
                                                        Margin="8,0,0,0"
                                                        FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                        Text="Stock Performance" />
                                                </StackPanel>
                                            </Button>
                                            <Button
                                                x:Name="CompareMaxBtn"
                                                Height="45"
                                                Margin="0"
                                                HorizontalContentAlignment="Stretch"
                                                Command="{Binding ShowMiscCommand}"
                                                CommandParameter="{Binding Path=Tag, ElementName=CompareMaxBtn}"
                                                FontSize="16"
                                                Foreground="{DynamicResource LightBrush}"
                                                Style="{StaticResource MaterialDesignFlatButton}"
                                                Tag="CarComparison|Misc|Max">
                                                <StackPanel
                                                    Margin="70,0,0,0"
                                                    HorizontalAlignment="Left"
                                                    Orientation="Horizontal">
                                                    <Viewbox Width="16" Height="16">
                                                        <materialDesign:PackIcon
                                                            Width="48"
                                                            Height="48"
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Top"
                                                            Kind="CarConnected" />
                                                    </Viewbox>
                                                    <TextBlock
                                                        Margin="8,0,0,0"
                                                        FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                        Text="Max Performance" />
                                                </StackPanel>
                                            </Button>
                                            <Button
                                                x:Name="CompareRankBtn"
                                                Height="45"
                                                Margin="0"
                                                HorizontalContentAlignment="Stretch"
                                                Command="{Binding ShowMiscCommand}"
                                                CommandParameter="{Binding Path=Tag, ElementName=CompareRankBtn}"
                                                FontSize="16"
                                                Foreground="{DynamicResource LightBrush}"
                                                Style="{StaticResource MaterialDesignFlatButton}"
                                                Tag="CarComparison|Misc|Rank">
                                                <StackPanel
                                                    Margin="70,0,0,0"
                                                    HorizontalAlignment="Left"
                                                    Orientation="Horizontal">
                                                    <Viewbox Width="16" Height="16">
                                                        <materialDesign:PackIcon
                                                            Width="48"
                                                            Height="48"
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Top"
                                                            Kind="CarConnected" />
                                                    </Viewbox>
                                                    <TextBlock
                                                        Margin="8,0,0,0"
                                                        FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                        Text="Star Ranks" />
                                                </StackPanel>
                                            </Button>
                                            <Button
                                                x:Name="CompareBlueprintsBtn"
                                                Height="45"
                                                Margin="0"
                                                HorizontalContentAlignment="Stretch"
                                                Command="{Binding ShowMiscCommand}"
                                                CommandParameter="{Binding Path=Tag, ElementName=CompareBlueprintsBtn}"
                                                FontSize="16"
                                                Foreground="{DynamicResource LightBrush}"
                                                Style="{StaticResource MaterialDesignFlatButton}"
                                                Tag="CarComparison|Misc|Blueprints">
                                                <StackPanel
                                                    Margin="70,0,0,0"
                                                    HorizontalAlignment="Left"
                                                    Orientation="Horizontal">
                                                    <Viewbox Width="16" Height="16">
                                                        <materialDesign:PackIcon
                                                            Width="48"
                                                            Height="48"
                                                            HorizontalAlignment="Left"
                                                            VerticalAlignment="Top"
                                                            Kind="CarConnected" />
                                                    </Viewbox>
                                                    <TextBlock
                                                        Margin="8,0,0,0"
                                                        FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                        Text="Blueprints" />
                                                </StackPanel>
                                            </Button>
                                        </StackPanel>
                                    </Expander>
                                </ItemsControl>
                            </StackPanel>
                        </Expander>
                        <!--#endregion-->
                        <!--#region About-->
                        <Expander
                            x:Name="About"
                            Margin="0"
                            Padding="2"
                            HorizontalContentAlignment="Stretch"
                            Background="{x:Null}"
                            Cursor="Hand"
                            FontFamily="{StaticResource RajdhaniMedium}"
                            FontSize="18"
                            Foreground="{DynamicResource LightBrush}"
                            TabIndex="6">
                            <Expander.Header>
                                <StackPanel
                                    Margin="10,0,0,0"
                                    HorizontalAlignment="Left"
                                    Orientation="Horizontal">
                                    <Viewbox Width="24" Height="24">
                                        <materialDesign:PackIcon Kind="Information" />
                                    </Viewbox>
                                    <TextBlock
                                        Margin="15,3,0,0"
                                        FontFamily="{DynamicResource RajdhaniSemiBold}"
                                        Text="About" />
                                </StackPanel>
                            </Expander.Header>
                            <StackPanel>
                                <ItemsControl x:Name="MiscIc1" HorizontalContentAlignment="Center">
                                    <Button
                                        x:Name="CreditsBtn"
                                        Height="45"
                                        Margin="0"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding ShowAboutCommand}"
                                        CommandParameter="{Binding Tag, ElementName=CreditsBtn}"
                                        FontSize="16"
                                        Foreground="{DynamicResource LightBrush}"
                                        Style="{StaticResource MaterialDesignFlatButton}"
                                        Tag="Credits|About">
                                        <StackPanel
                                            Margin="60,0,0,0"
                                            HorizontalAlignment="Left"
                                            Orientation="Horizontal">
                                            <Viewbox Width="16" Height="16">
                                                <materialDesign:PackIcon
                                                    Width="48"
                                                    Height="48"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Top"
                                                    Kind="BookInformationVariant" />
                                            </Viewbox>
                                            <TextBlock
                                                Margin="8,0,0,0"
                                                FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                Text="Credits" />
                                        </StackPanel>
                                    </Button>
                                    <Button
                                        x:Name="AboutBtn"
                                        Height="45"
                                        Margin="0"
                                        HorizontalContentAlignment="Stretch"
                                        Command="{Binding ShowAboutCommand}"
                                        CommandParameter="{Binding Tag, ElementName=AboutBtn}"
                                        FontSize="16"
                                        Foreground="{DynamicResource LightBrush}"
                                        Style="{StaticResource MaterialDesignFlatButton}"
                                        Tag="About|About">
                                        <StackPanel
                                            Margin="60,0,0,0"
                                            HorizontalAlignment="Left"
                                            Orientation="Horizontal">
                                            <Viewbox Width="16" Height="16">
                                                <materialDesign:PackIcon
                                                    Width="48"
                                                    Height="48"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Top"
                                                    Kind="BookInformationVariant" />
                                            </Viewbox>
                                            <TextBlock
                                                Margin="8,0,0,0"
                                                FontFamily="{DynamicResource RajdhaniSemiBold}"
                                                Text="About" />
                                        </StackPanel>
                                    </Button>
                                </ItemsControl>
                            </StackPanel>
                        </Expander>
                        <!--#endregion-->
                    </StackPanel>
                </ScrollViewer>
                <!--#endregion-->
                <TextBlock
                    Grid.Row="0"
                    Margin="25,0,0,20"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Bottom"
                    FontFamily="{StaticResource RajdhaniSemiBold}"
                    FontSize="20"
                    Foreground="{StaticResource LightBrush}"
                    Text="{Binding Source={x:Static system:DateTime.Now}, StringFormat=D}" />
            </Grid>
            <!--#endregion-->
            <!--#region Frame-->
            <Grid
                Grid.Row="0"
                Grid.RowSpan="3"
                Grid.Column="1"
                Margin="0,100,0,70"
                Background="{StaticResource DarkerBrush}">
                <ContentControl
                    x:Name="MainFrame"
                    Width="1370"
                    Height="860"
                    Content="{Binding CurrentViewModel}" />
            </Grid>
            <!--#endregion-->
        </Grid>
    </materialDesign:DialogHost>

</Window>
